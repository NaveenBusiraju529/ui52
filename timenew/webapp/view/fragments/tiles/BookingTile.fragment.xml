<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:viz="sap.viz.ui5.controls"
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
    xmlns:viz.data="sap.viz.ui5.data"
    xmlns:core="sap.ui.core">
<GenericTile id="myBookingTile" 
    header="My Booking"
    subheader="{bookingModel>/month}"
    frameType="TwoByOne" 
    press=".onMyBookingTilePress">
    <TileContent footer="Project Allocation">
        <viz:VizFrame id="bookingVizFrame" 
                   height="18rem"
                   width="100%"
                   vizType="bar">
            <viz:dataset>
                <viz.data:FlattenedDataset data="{bookingModel>/projects}">
                    <viz.data:dimensions>
                        <viz.data:DimensionDefinition name="Project" value="{projectName}" />
                    </viz.data:dimensions>
                    <viz.data:measures>
                        <viz.data:MeasureDefinition name="Filled Days" value="{filledDays}" />
                        <viz.data:MeasureDefinition name="Allocated Days" value="{allocatedDays}" />
                    </viz.data:measures>
                </viz.data:FlattenedDataset>
            </viz:dataset>
            <viz:feeds>
                <viz.feeds:FeedItem uid="valueAxis" type="Measure" values="Filled Days,Allocated Days" />
                <viz.feeds:FeedItem uid="categoryAxis" type="Dimension" values="Project" />
            </viz:feeds>
        </viz:VizFrame>
    </TileContent>
</GenericTile>
</mvc:View>